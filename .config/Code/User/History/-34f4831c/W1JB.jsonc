{
    "layer": "top",
    "position": "top",
    "modules-left": [
        "cpu",
        "memory",
        "temperature",
        "network#speed",
    ],
    "modules-center": [],
    "modules-right": [
        "tray",
        "clock",
        "pulseaudio",
        "battery",
        "bluetooth",
        "network",
    ],
    "reload_style_on_change": true,
    "memory": {
        "format": "Óâ¶ {percentage}%"
    },
    "cpu": {
        "interval": 1,
        "format": "Û∞çõ {usage}%",
        "format-icons": [
            "‚ñÅ",
            "‚ñÇ",
            "‚ñÉ",
            "‚ñÑ",
            "‚ñÖ",
            "‚ñÜ",
            "‚ñá",
